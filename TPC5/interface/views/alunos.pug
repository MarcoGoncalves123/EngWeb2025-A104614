extends layout

block content
    .w3-card-4 
        header.w3-container.w3-blue 
            h1= tit
        .w3.container
            table.w3-table-all 
                tr 
                    th Identificador
                    th Nome 
                    th GitLink
                each aluno in alunos 
                    tr 
                        td
                            a(href=`/alunos/${aluno._id}`)= aluno._id
                        td= aluno.nome
                        td= aluno.git          
        footer.w3-container.w3-blue
            adress Generated by EW2025 class 


